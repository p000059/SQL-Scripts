-- =============================================
-- Author:		Marcelo Guima
-- Create date: 18/04/2020
-- Description:	Atualiza tblValorUST
-- =============================================
ALTER PROCEDURE [dbo].[spUpdatetblValorUST]
(
	@pkValorUST int,
	@vlUST int
)
AS
BEGIN
	UPDATE	tblValorUST
	SET		vlUST = @vlUST,
			dtAtualizacao = GETDATE()
	WHERE	pkValorUST = @pkValorUST
END

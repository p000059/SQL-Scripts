-- =============================================
-- Author:		Marcelo Guima
-- Create date: 18/04/2020
-- Description:	Atualiza tblValorUST
-- =============================================
CREATE PROCEDURE spAtualizaValorUST
(
	@pkValorUST int,
	@vlUST int
)
AS
BEGIN
	UPDATE	tblValorUST
	SET		vlUST = @vlUST			
	WHERE	pkValorUST = @pkValorUST
END
GO
